    "import imageio\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "# 创建一个视频写入器\n",
    "with imageio.get_writer('animation_fps_1.mp4', fps=1) as writer:\n",
    "    for i in np.arange(0, 300 + 1, 0.5):\n",
    "        filename = f\"animi_{i}.png\"\n",
    "        image = imageio.imread(filename)\n",
    "        writer.append_data(image)"
